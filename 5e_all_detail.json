{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"$id":"5e_all_detail.json",
	"type": "object",
	"description": "All 5e data with extended properties",
	"properties": {
		"characters": {
			"type": "array",
			"definition": "Pregenerated characters",
			"items": {
				"$ref": "character.json"
			}
		},
		"classes": {
			"type": "array",
			"items": {
				"$ref": "class_detail.json"
			}
		},
		"feats": {
			"type": "array",
			"items": {
				"$ref": "feat_detail.json#"
			}
		},
		"items": {
			"type": "array",
			"items": {
				"$ref": "item_detail.json"
			}
		},
		"monsters":{
			"type": "array",
			"items": {
				"$ref": "monster_detail.json#"
			}
		},
		"races": {
			"type": "array",
			"items": {
				"$ref": "race_detail.json"
			}
		},
		"spells": {
			"type": "array",
			"items": {
				"$ref": "spell_detail.json"
			}
		}
	},
	"additionalProperties": false
}